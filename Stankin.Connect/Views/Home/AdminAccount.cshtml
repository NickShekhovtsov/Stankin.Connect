@model List<ContactToAdmin>


<!DOCTYPE html>

<html lang="ru">

<head>
    <title>message</title>
    <link rel="icon" href="/img/stankinLogo.png">

    <meta http-equiv="Content-type" content="text/html; charset=UTF-8" />

    <link rel="stylesheet" type="text/css" href="/css/style.css">
</head>


<body class="bg">
    <form action="#" method="post" enctype="multipart/form-data"></form>

    <div class="adminInput">
        <br>

        <button class="btn" onclick="Back();">Выйти из сообщений</button>

        <div class="message">

            @foreach (var report in Model)
            {

                <div class="blockMessage">
                    <span class="space">@report.mail</span>
                    <span class="space">@report.text</span>
                </div>

            }
            

        </div>

    </div>



    <script type="text/javascript">

        function Back() {
            window.open('/Home/AdminPanel', '_self');
        }

    </script>

</body>
</html>


